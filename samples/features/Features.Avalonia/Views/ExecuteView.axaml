<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Features.Avalonia.Views.ExecuteView">
    <Grid>
        <StackPanel Margin="40,20">
            <CheckBox Name="Safe" Content="Safe (use Execute.OnUIThread)"/>
            <Button Name="StartBackgroundWork" Content="Start background work"/>
            <TextBlock Name="Output"/>
            <TextBlock Name="tbTest"></TextBlock>
        </StackPanel>
    </Grid>
</UserControl>
